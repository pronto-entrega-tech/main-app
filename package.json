{
  "name": "main-api",
  "author": "ProntoEntrega",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "postinstall": "node scripts/shim.js",
    "postuninstall": "yarn postinstall",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "next dev -p 3001",
    "web:https": "yarn https-envs node scripts/server.js",
    "https-envs": "cross-env NODE_EXTRA_CA_CERTS=\"$(mkcert -CAROOT)/rootCA.pem\" NEXT_PUBLIC_HTTPS=true",
    "use-dev-api": "cross-env NEXT_PUBLIC_USE_DEV_API=true",
    "use-local-ip": "cross-env NEXT_PUBLIC_LOCAL_IP=$(node -p \"require('os').networkInterfaces().en0.find((v) => v.family === 'IPv4').address\")",
    "lint": "next lint -d src -d pages",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:dev": "yarn test --watch",
    "android:build": "expo build:android",
    "ios:build": "expo build:ios",
    "web:build": "next build",
    "android:prod": "expo run:android",
    "ios:prod": "expo run:ios",
    "web:prod": "next start -p 3001"
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "jest": {
    "preset": "jest-expo/universal"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-community/netinfo": "8.2.0",
    "@react-native-community/slider": "4.2.1",
    "@react-native-picker/picker": "2.4.0",
    "@react-navigation/material-bottom-tabs": "^6.2.1",
    "@react-navigation/material-top-tabs": "^6.2.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/stack": "^6.2.1",
    "axios": "^0.26.1",
    "date-fns": "^2.28.0",
    "expo": "^45.0.4",
    "expo-auth-session": "~3.6.1",
    "expo-clipboard": "~3.0.1",
    "expo-document-picker": "~10.2.1",
    "expo-font": "~10.1.0",
    "expo-image-picker": "~13.1.1",
    "expo-linking": "~3.1.0",
    "expo-location": "~14.2.2",
    "expo-notifications": "~0.15.4",
    "expo-random": "~12.2.0",
    "expo-splash-screen": "~0.15.1",
    "expo-status-bar": "~1.3.0",
    "expo-system-ui": "~1.2.0",
    "next": "^12.3.1",
    "next-pwa": "^5.6.0",
    "qs": "^6.10.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-elements": "^3.4.2",
    "react-native-gesture-handler": "~2.2.1",
    "react-native-pager-view": "5.4.15",
    "react-native-paper": "^4.12.0",
    "react-native-qrcode-svg": "^6.1.2",
    "react-native-reanimated": "~2.8.0",
    "react-native-safe-area-context": "4.2.4",
    "react-native-screens": "~3.11.1",
    "react-native-svg": "12.3.0",
    "react-native-tab-view": "^3.1.1",
    "react-native-web": "0.17.7",
    "scheduler": "^0.22.0",
    "socket.io-client": "^4.5.1",
    "styled-components": "^5.3.5",
    "use-context-selector": "^1.3.10"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@expo/next-adapter": "^4.0.12",
    "@next/bundle-analyzer": "^12.3.1",
    "@types/jest": "^26.0.24",
    "@types/qs": "^6.9.7",
    "@types/react": "~17.0.21",
    "@types/react-dom": "~17.0.11",
    "@types/react-native": "~0.67.6",
    "@types/styled-components": "^5.1.25",
    "@types/styled-components-react-native": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "babel-plugin-styled-components": "^2.0.7",
    "babel-plugin-transform-imports": "^2.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.13.0",
    "eslint-config-next": "^12.1.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prefer-arrow-functions": "^3.1.4",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^26.6.3",
    "jest-expo": "^45.0.1",
    "next-compose-plugins": "^2.2.1",
    "next-fonts": "^1.5.1",
    "next-transpile-modules": "^9.0.0",
    "prettier": "^2.6.2",
    "typescript": "~4.3.5"
  }
}
