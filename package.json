{
  "name": "main-app",
  "author": "ProntoEntrega",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "prebuild": "expo prebuild --clean --platform android",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start-android": "expo start --dev-client --android",
    "start-ios": "expo start --dev-client --ios",
    "web": "next dev -p 3001",
    "web:prod": "next start -p 3001",
    "web:https": "pnpm https-envs node scripts/https-server.js",
    "https-envs": "cross-env NODE_EXTRA_CA_CERTS=\"$(mkcert -CAROOT)/rootCA.pem\" NEXT_PUBLIC_HTTPS=true",
    "lint": "next lint -d src -d pages",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:dev": "pnpm test --watch",
    "android:build": "eas build --local --platform android",
    "ios:build": "eas build --local --platform ios",
    "web:build": "next build"
  },
  "jest": {
    "preset": "jest-expo/universal"
  },
